{
  "name": "@rdmr-eu/sparql-query-runner",
  "version": "3.2.0",
  "description": "Run SPARQL queries serially on a SPARQL endpoint.",
  "main": "dist/cli/index.js",
  "repository": "https://github.com/redmer/sparql-query-runner.git",
  "author": "Redmer Kronemeijer <12477216+redmer@users.noreply.github.com> (https://rdmr.eu/)",
  "license": "MPL-2.0",
  "private": false,
  "bin": "dist/cli/index.js",
  "scripts": {
    "test": "jest",
    "build": "tsc -p ./tsconfig-build.json",
    "watch": "tsc --watch -p ./tsconfig-build.json",
    "util:lint": "cross-env ESLINT_STRICT=true ESLINT_EXPENSIVE=true eslint",
    "prepare": "yarn build",
    "prepublishOnly": "yarn test && yarn run util:lint",
    "preversion": "yarn run util:lint",
    "postversion": "git push && git push --tags"
  },
  "files": [
    "dist/**/*"
  ],
  "types": "dist",
  "type": "module",
  "devDependencies": {
    "@types/command-exists": "^1.2.0",
    "@types/fs-extra": "^9.0.12",
    "@types/jest": "^29.2.0",
    "@types/n3": "^1.10.4",
    "@types/node": "^18.11.3",
    "@types/node-fetch": "^2.5.11",
    "@types/rdf-ext": "^1.3.11",
    "@types/rdf-validate-shacl": "^0.4.0",
    "@types/rdfjs__parser-n3": "^1.1.5",
    "cross-env": "^7.0.3",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^27.1.3",
    "jest": "^29.2.1",
    "lint-staged": "^13.0.3",
    "msw": "^0.47.4",
    "openapi-backend": "^5.5.0",
    "prettier": "^2.3.1",
    "ts-essentials": "^9.0.0",
    "ts-jest": "^29.0.3",
    "typescript": "^4.3.2"
  },
  "lint-staged": {
    "*.ts?(x)": [
      "prettier --parser typescript --write",
      "yarn run util:lint",
      "yarn test --findRelatedTests"
    ],
    "*.json": [
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ],
    "*.yml": [
      "prettier --write"
    ]
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/(*.)+test\\.ts",
      "**/(*.)+test\\.js"
    ],
    "transform": {
      "\\.ts$": "ts-jest"
    }
  },
  "prettier": {
    "printWidth": 100
  },
  "dependencies": {
    "@comunica/query-sparql": "^2.5.0",
    "@comunica/query-sparql-file": "^2.4.3",
    "@rdfjs/parser-n3": "^2.0.0",
    "@stichting-crow/laces-hub-js": "^1.0.0",
    "@types/lodash": "^4.14.186",
    "chalk": "^5.1.2",
    "command-exists": "^1.2.9",
    "dotenv": "^16.0.3",
    "fast-csv": "^4.3.6",
    "fs-extra": "^10.0.0",
    "lodash": "^4.17.21",
    "mdb-reader": "^2.2.4",
    "n3": "^1.10.0",
    "node-fetch": "^3.2.10",
    "rdf-ext": "^2.1.0",
    "rdf-validate-shacl": "^0.4.5",
    "yaml": "^2.1.3",
    "yargs": "^17.6.0"
  }
}
