{
  "name": "@rdmr-eu/sparql-query-runner",
  "version": "4.0.0-0",
  "description": "Run SPARQL queries serially on a SPARQL endpoint.",
  "main": "dist/cli/cli.js",
  "repository": "https://github.com/redmer/sparql-query-runner.git",
  "author": "Redmer Kronemeijer <12477216+redmer@users.noreply.github.com> (https://rdmr.eu/)",
  "license": "MPL-2.0",
  "private": false,
  "bin": {
    "sparql-query-runner": "dist/cli/cli.js"
  },
  "scripts": {
    "test": "jest",
    "build": "tsc -p ./tsconfig.json",
    "watch": "tsc --watch -p ./tsconfig.json",
    "lint": "cross-env ESLINT_STRICT=true ESLINT_EXPENSIVE=true eslint . --ext .ts",
    "prepare": "yarn build",
    "prepublishOnly": "yarn test && yarn run lint",
    "preversion": "yarn run lint",
    "postversion": "git push && git push --tags"
  },
  "files": [
    "dist/**/*"
  ],
  "types": "dist",
  "type": "module",
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/n3": "^1.10.4",
    "@types/node": "^18.11.3",
    "@types/node-fetch": "^2.5.11",
    "@types/rdf-validate-shacl": "^0.4.0",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^27.1.3",
    "jest": "^29.4.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.3.1",
    "ts-essentials": "^9.0.0",
    "ts-jest": "^29.0.3",
    "typescript": "^4.3.2"
  },
  "lint-staged": {
    "*.ts?(x)": [
      "prettier --parser typescript --write",
      "yarn run lint",
      "yarn test --findRelatedTests"
    ],
    "*.json": [
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ],
    "*.yml": [
      "prettier --write"
    ]
  },
  "jest": {
    "preset": "ts-jest/presets/default-esm",
    "testEnvironment": "node",
    "testMatch": [
      "**/(*.)+test\\.ts"
    ],
    "transform": {
      "\\.[tj]sx?$": [
        "ts-jest",
        {
          "useESM": true
        }
      ]
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "extensionsToTreatAsEsm": [
      ".ts"
    ]
  },
  "prettier": {
    "printWidth": 100
  },
  "dependencies": {
    "@comunica/query-sparql": "^2.5.0",
    "@comunica/types": "^2.6.8",
    "@rdfjs/types": "^1.1.0",
    "@rdmr-eu/rdfjs-source-msaccess": "^1.1.2",
    "@triply/triplydb": "^4.3.0",
    "chalk": "^5.1.2",
    "dotenv": "^16.0.3",
    "n3": "^1.10.0",
    "node-fetch": "^3.2.10",
    "rdf-validate-shacl": "^0.4.5",
    "sparqljs": "^3.6.1",
    "yaml": "^2.1.3",
    "yargs": "^17.6.0"
  }
}
