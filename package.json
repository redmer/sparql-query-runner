{
  "name": "@rdmr-eu/sparql-query-runner",
  "version": "2.0.7",
  "description": "Run SPARQL queries serially on a SPARQL endpoint.",
  "main": "dist/cli/index.js",
  "repository": "https://github.com/redmer/sparql-query-runner.git",
  "author": "Redmer Kronemeijer <12477216+redmer@users.noreply.github.com> (https://rdmr.eu/)",
  "license": "MPL-2.0",
  "private": false,
  "bin": "dist/cli/index.js",
  "scripts": {
    "test": "jest --passWithNoTests",
    "build": "tsc -p ./tsconfig-build.json",
    "util:lint": "cross-env ESLINT_STRICT=true ESLINT_EXPENSIVE=true eslint",
    "prepare": "yarn build",
    "prepublishOnly": "yarn test && yarn run util:lint",
    "preversion": "yarn run util:lint",
    "postversion": "git push && git push --tags"
  },
  "files": [
    "dist/**/*"
  ],
  "types": "dist",
  "devDependencies": {
    "@types/command-exists": "^1.2.0",
    "@types/fs-extra": "^9.0.12",
    "@types/jest": "^26.0.24",
    "@types/n3": "^1.10.4",
    "@types/node": "^15.12.1",
    "@types/node-fetch": "^2.5.11",
    "@types/rdf-ext": "^1.3.11",
    "@types/rdf-validate-shacl": "^0.2.4",
    "@types/rdfjs__parser-n3": "^1.1.5",
    "cross-env": "^7.0.3",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.3.6",
    "jest": "^27.0.6",
    "lint-staged": "^11.0.0",
    "msw": "^0.31.0",
    "openapi-backend": "^4.1.0",
    "prettier": "^2.3.1",
    "ts-essentials": "^9.0.0",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.2"
  },
  "lint-staged": {
    "*.ts?(x)": [
      "prettier --parser typescript --write",
      "yarn run util:lint",
      "yarn test --findRelatedTests"
    ],
    "*.json": [
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ],
    "*.yml": [
      "prettier --write"
    ]
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/(*.)+test\\.ts"
    ],
    "transform": {
      "\\.ts$": "ts-jest"
    }
  },
  "prettier": {
    "printWidth": 100
  },
  "dependencies": {
    "@rdfjs/parser-n3": "^1.1.4",
    "@stichting-crow/laces-hub-js": "^0.5.6",
    "command-exists": "^1.2.9",
    "dotenv": "^10.0.0",
    "fast-csv": "^4.3.6",
    "fs-extra": "^10.0.0",
    "mdb-reader": "^1.0.0",
    "n3": "^1.10.0",
    "node-fetch": "^2.6.1",
    "rdf-ext": "^1.3.5",
    "rdf-validate-shacl": "^0.3.2",
    "yargs": "^17.2.1"
  }
}
