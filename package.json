{
  "name": "@rdmr-eu/sparql-query-runner",
  "version": "5.0.0",
  "description": "Run SPARQL queries serially on a SPARQL endpoint.",
  "main": "dist/cli/cli.js",
  "repository": "https://github.com/redmer/sparql-query-runner.git",
  "author": "Redmer Kronemeijer <12477216+redmer@users.noreply.github.com> (https://rdmr.eu/)",
  "license": "MPL-2.0",
  "private": false,
  "bin": {
    "sparql-query-runner": "dist/cli/cli.js"
  },
  "scripts": {
    "test": "jest",
    "build": "tsc -p ./tsconfig.json",
    "watch": "tsc --watch -p ./tsconfig.json",
    "lint": "cross-env ESLINT_STRICT=true ESLINT_EXPENSIVE=true eslint . --ext .ts",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test && npm run lint",
    "preversion": "npm run lint",
    "postversion": "git push && git push --tags"
  },
  "files": [
    "dist/**/*"
  ],
  "types": "dist",
  "type": "module",
  "devDependencies": {
    "@types/command-exists": "^1.2.0",
    "@types/jest": "^29.4.0",
    "@types/json-stable-stringify": "^1.0.34",
    "@types/n3": "^1.10.4",
    "@types/node": "^20.0.0",
    "@types/node-fetch": "^2.5.11",
    "@types/rdf-validate-shacl": "^0.4.0",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "asynciterator": "^3.8.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.25.0",
    "eslint-config-eslint": "^9.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^27.1.3",
    "jest": "^29.4.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.3.1",
    "ts-jest": "^29.0.3",
    "typescript": "^5.0.0"
  },
  "lint-staged": {
    "*.ts?(x)": [
      "prettier --parser typescript --write",
      "npm run lint",
      "npm test --findRelatedTests"
    ],
    "*.json": [
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ],
    "*.yml": [
      "prettier --write"
    ]
  },
  "jest": {
    "preset": "ts-jest/presets/default-esm",
    "testEnvironment": "node",
    "testMatch": [
      "**/(*.)+test\\.ts"
    ],
    "transform": {
      "\\.[tj]sx?$": [
        "ts-jest",
        {
          "useESM": true
        }
      ]
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "extensionsToTreatAsEsm": [
      ".ts"
    ]
  },
  "prettier": {
    "printWidth": 100
  },
  "eslintConfig": {
    "env": {
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "overrides": [],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "plugins": [
      "@typescript-eslint"
    ],
    "rules": {
      "@typescript-eslint/ban-ts-comment": 0,
      "@typescript-eslint/no-unused-vars": [
        "error",
        {
          "argsIgnorePattern": "^_"
        }
      ]
    }
  },
  "dependencies": {
    "@comunica/query-sparql": "^2.5.0",
    "@comunica/types": "^2.6.8",
    "@rdfjs/types": "^1.1.0",
    "@triply/triplydb": "^4.3.0",
    "chalk": "^5.1.2",
    "command-exists": "^1.2.9",
    "dotenv": "^16.0.3",
    "json-stable-stringify": "^1.0.2",
    "n3": "^1.10.0",
    "node-fetch": "^3.2.10",
    "rdf-data-factory": "^1.1.2",
    "rdf-parse": "^2.3.2",
    "rdf-store-stream": "^2.0.0",
    "rdf-stores": "^1.0.0",
    "rdf-validate-shacl": "^0.4.5",
    "sparqljs": "^3.6.1",
    "yaml": "^2.1.3",
    "yargs": "^17.6.0"
  }
}
